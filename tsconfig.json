{
  "compilerOptions": {
    "lib":["ES2021"],
    "module":"ESNext",
    "target":"ES2020",
    "moduleResolution":"node",
    "esModuleInterop":true,
    "strict":true,
    "skipLibCheck":true,
    "resolveJsonModule":true,
    "useDefineForClassFields":true,
    "declaration":true,
    "types":["node","express"]
  },
  "include":["src/**/*.ts"],
  "exclude":["src/web-server.js"]
}